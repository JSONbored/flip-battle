# Deployment Configuration
# Copy this file to .env and fill in your values

# Private key for deployment (WITHOUT 0x prefix)
PRIVATE_KEY=your_private_key_here

# Chainlink VRF Subscription ID
# Create at: https://vrf.chain.link
# Base Sepolia: Create subscription on Base Sepolia testnet
# Base Mainnet: Create subscription on Base Mainnet
VRF_SUBSCRIPTION_ID=0

# RPC URLs
BASE_SEPOLIA_RPC_URL=https://sepolia.base.org
BASE_MAINNET_RPC_URL=https://mainnet.base.org

# Basescan API Key for contract verification
# Get from: https://basescan.org/myapikey
BASESCAN_API_KEY=your_basescan_api_key_here

# Etherscan API Key (alternative, Basescan uses same API)
ETHERSCAN_API_KEY=your_etherscan_api_key_here
